<section>




    <h1>Hello</h1>

    <form id="loginForm" ng-submit="ctrl.submitIt()">
      <label for="email">Email</label>
      <input type="text" name="email" id="email" ng-model="ctrl.email">
      <label for="password">Password</label>
      <input type="password" name="password" id="password" ng-model="ctrl.password">
      <input type="submit" value="Sign In">
    </form>

</section>


<div>
<p> Sign Up Below </p>
    <form id="newUser">
      <label for="email">Email</label>
      <input type="text" name="email" id="new-email">
      <label for="name">Name</label>
      <input type="text" name="name" id="new-name">
      <label for="location">Location</label>
      <input type="text" name="location" id="new-location">
      <input type="submit" value="Add User">
    </form>

</div>






  </body>
